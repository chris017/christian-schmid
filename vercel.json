{
    "version": 2,
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html",
        "continue": true
      },
      {
        "src": "/dashboard/blog/.*",
        "dest": "/",
        "continue": true
      },
      {
        "src": "/dashboard/blog",
        "dest": "/",
        "continue": true
      },
      {
        "src": "/",
        "dest": "/",
        "continue": true
      },
      {
        "src": "/dashboard/",
        "dest": "/",
        "continue": true
      }
    ]
  }